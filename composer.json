{
    "name": "hubculture/fireblocks-php-sdk",
    "description": "PHP SDK for Fireblocks API",
    "type": "library",
    "authors": [
        {
            "name": "Tharanga Kothalawala",
            "email": "tharanga.kothalawala@gmail.com"
        }
    ],
    "require": {
        "ext-json": "*",
        "guzzlehttp/psr7": "^1.5@dev",
        "guzzlehttp/guzzle": "^6.3@dev",
        "firebase/php-jwt": "^v5.4"
    },
    "autoload": {
